{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "boilerplate",
  "main": "./src/app.js",
  "scripts": {
    "test": "npm run test",
    "prestart": "babel ./src --out-dir build",
    "start": "node --max_old_space_size=4096 ./build/app",
    "dev": "nodemon --exec babel-node ./src/app.js",
    "eslint": "npx eslint ./src",
    "eslint-fix": "npx eslint ./src --fix",
    "db:migrate": "npx sequelize-cli db:migrate",
    "make:seed": "npx sequelize-cli seed:generate --name ",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:undo-all": "npx sequelize-cli db:migrate:undo:all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.21.0",
    "babel-register": "^6.26.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.21",
    "sequelize-cli": "^6.6.0"
  }
}
